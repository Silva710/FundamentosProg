int main(int argc, char** argv) {
    int y;
    int x;

    for(y=1; y<=10; y++){ 

     printf ("///////////\n");

         for (x=1; x<=10;x++){

    printf ("%d X %d = %d\n",y,x,y*x);



         }

    }
    return (EXIT_SUCCESS);
}

